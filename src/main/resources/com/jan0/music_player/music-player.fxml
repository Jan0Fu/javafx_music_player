<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="115.0" prefWidth="644.0" style="-fx-background-color: #333333;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jan0.music_player.Controller">
   <children>
      <Label fx:id="songLabel" alignment="CENTER" prefHeight="93.0" prefWidth="644.0" text="MP3 Player" textFill="#66ae3f">
         <font>
            <Font size="63.0" />
         </font>
      </Label>
      <ProgressBar fx:id="songProgressBar" layoutY="93.0" prefHeight="16.0" prefWidth="644.0" progress="0.0" />
      <Button fx:id="playButton" layoutY="109.0" mnemonicParsing="false" onAction="#playMedia" prefHeight="36.0" prefWidth="84.0" text="PLAY">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="pauseButton" layoutX="84.0" layoutY="109.0" mnemonicParsing="false" onAction="#pauseMedia" prefHeight="36.0" prefWidth="84.0" text="PAUSE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="resetButton" layoutX="168.0" layoutY="109.0" mnemonicParsing="false" onAction="#resetMedia" prefHeight="36.0" prefWidth="84.0" text="RESET">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="prevButton" layoutX="252.0" layoutY="109.0" mnemonicParsing="false" onAction="#prevMedia" prefHeight="36.0" prefWidth="84.0" text="PREV">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="nextButton" layoutX="336.0" layoutY="109.0" mnemonicParsing="false" onAction="#nextMedia" prefHeight="36.0" prefWidth="84.0" text="NEXT">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <ComboBox fx:id="speedBox" layoutX="420.0" layoutY="109.0" onAction="#changeSpeed" prefHeight="36.0" prefWidth="84.0" promptText="SPEED" />
      <Slider fx:id="volumeSlider" layoutX="504.0" layoutY="119.0" max="200.0" value="100.0" />
   </children>
</AnchorPane>
